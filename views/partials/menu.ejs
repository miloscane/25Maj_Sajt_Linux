				</div>
				<script>
					function openMenu(elem){
						var menuOpened	=	Number(elem.dataset.opened);
						if(menuOpened==0){
							//open the menu
							document.getElementsByClassName("menu")[0].classList.add("menuOpened")
							document.getElementById("menuIcon").src = "/icons/menu-opened-yellow.png?<%=fileVersion%>";
							elem.dataset.opened = 1;
						}else{
							//close the menu
							document.getElementsByClassName("menu")[0].classList.remove("menuOpened");
							document.getElementById("menuIcon").src = "/icons/menu-closed-yellow.png?<%=fileVersion%>";
							elem.dataset.opened = 0;

						}
					}
				</script>
