				<script>
					function responsiveOpener(elem){
						if(document.getElementsByClassName("menu")[0].classList.contains("menuOpened")){
							if(elem.classList.contains("subMenuOpened")){
								elem.classList.remove("subMenuOpened");
								elem.parentElement.getElementsByClassName("subMenu")[0].style.display="none";
							}else{
								elem.classList.add("subMenuOpened");
								elem.parentElement.getElementsByClassName("subMenu")[0].style.display="block";

